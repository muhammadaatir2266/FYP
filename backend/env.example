# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration (PostgreSQL)
DATABASE_URL="postgresql://username:password@localhost:5432/medical_assistant?schema=public"

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# VAPI Configuration (for calling agent)
VAPI_API_KEY=your_vapi_api_key_here
VAPI_PHONE_NUMBER=your_vapi_phone_number_here

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here
JWT_EXPIRES_IN=7d

# ML Model API (Python Flask/FastAPI)
ML_MODEL_API_URL=http://localhost:8000

# n8n Webhook URLs (for workflow automation)
N8N_WEBHOOK_APPOINTMENT=http://localhost:5678/webhook/appointment
N8N_WEBHOOK_CALL=http://localhost:5678/webhook/call
N8N_WEBHOOK_NOTIFICATION=http://localhost:5678/webhook/notification

